    "def resize_image(input_path, width = 768 , height = 768):\n",
    "    from PIL import Image\n",
    "    # Load the image\n",
    "    img = Image.open(input_path)\n",
    "\n",
    "    # Resize the image\n",
    "    img_resized = img.resize((width, height))\n",
    "\n",
    "    # Save the resized image\n",
    "    img_resized.save(input_path)\n"
   ]
